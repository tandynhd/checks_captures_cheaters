{
    "manifest_version": 3,
    "name": "Chess.com Rating Viewer",
    "version": "1.0",
    "description": "View Chess.com user ratings and most played moves",
    "permissions": ["activeTab", "storage", "https://api.chess.com/"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  